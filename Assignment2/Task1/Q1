
def myreduce(fun, alist):
    x = alist[0]
    for i in range(1, len(alist)):
        x = fun(x, alist[i])
    return x


def summ(x1, x2):
    return x1 + x2


def Product(x1, x2):
    return(x1 * x2)


a = [1, 2, 3, 4, 5]
print("Sum of all element in list a: ", myreduce(summ, a))
print("Product of all elements in list a: ", myreduce(Product, a))
